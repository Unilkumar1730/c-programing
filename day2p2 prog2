#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int R, C, T;
    scanf("%d %d %d", &R, &C, &T);
    
    int seconstart = (2 - 1) * R + 1;
    int seconend = 2 * R;

    int lastonestart = (C - 1 - 1) * R + 1;
    int lastoneend = (C - 1) * R;

    if ((T >= secondstart && T <= seconend) ||
        (T >= laststart && T <= lastoneend)) {
        printf("It is a mango tree\n");
    } else {
        printf("It is not a mango tree\n");
    }

    return 0;
}
