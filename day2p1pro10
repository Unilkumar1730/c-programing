#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    int a;
    scanf("%d",&a);
    if(a>=1&&a<=9){
    int b = a*a;
        
    int c = b%10;
        
    if (c==a )
        
        printf("Automorphic Number");
        
    else if (c!=a )
        printf("Not Automorphic Number");
    }
    else
        printf("Invalid Input");
    return 0;
}
