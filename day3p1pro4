#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
 int n;
    scanf("%d", &n);
    
    int arr[n];
    int sum = 0;
    
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sum += arr[i];
    }

    float mean = (float)sum / n;
    printf("Array Mean Value is %.2f\n", mean);

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
