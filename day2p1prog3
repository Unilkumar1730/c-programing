#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
           int n;
    scanf("%d",&n);
    if(n>0)
    {
        printf("The Given Number %d is Positive.",n);
        
    }
    else if(n<0){
        printf("The Given Number %d is Negative.",n);
    }
     else if(n==0){
        printf("The Given Number 0 is Zero.");
    }
        
    return 0;
}
