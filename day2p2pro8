#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
  int a,b,c,d;
    scanf("%d %d %d %d",&a,&b,&c,&d);
    if(b>2)
        printf("Invalid Input");
    else if (b==1)
    {
        d = a+c;
        printf("%d\n",d);
        
}
    else if (b==2 && a>c){
        d = a-c;
          printf("%d\n",d);
        
    }
       else if (b==2 && a<c){
           printf("Insufficient Balance");
}
    
        
    return 0;
}
